{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "viam:customlinux",
  "visibility": "public",
  "url": "https://github.com/viam-modules/customlinux",
  "description": "Go module for customlinux customlinux board, compatible with Viam",
  "models": [
    {
      "api": "rdk:component:board",
      "model": "viam:customlinux:customlinux",
      "markdown_link": "README.md#configure-your-customlinux-board",
      "short_description": "board model for any board that runs a linux OS and is not implemented by viam-modules"
    }
  ],
  "build": {
    "build": "make module",
    "path": "bin/module.tar.gz",
    "arch" : ["linux/arm64", "linux/amd64"]
  },
  "entrypoint": "bin/customlinux"
}
